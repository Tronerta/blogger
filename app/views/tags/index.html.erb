<h1>Tags:</h1>

<ul id="tags">
	<% @tags.each do |tag| %>
		<li>
			<%= link_to tag.name, tag_path(tag.id) %><br>
			<%= link_to "delete tag", tag_path(tag.id), method: :delete, data: {confirm: "Really delete the tag?"} %>
		</li><br>
		<hr>
	<% end %>
</ul>

<%= link_to "Home", root_path, class: "new_article" %>
